{
  "expo": {
    "name": "Planner",
    "slug": "planner",
    "version": "1.0.0",
    "scheme": "Planner",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2f7bf5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ksroyal1010"
    },
    "android": {

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ks_royal.planner"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8d39e076-dd83-4ff3-9900-39c56d724958"
      }
    },
    "owner": "ks_royal"
  }
}
